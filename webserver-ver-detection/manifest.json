{
   "manifest_version": 2,
   "name": "Webserver Version Detection",
   "version": "0.1", 
   "description": "Displays a list of the last 10 (configurable) potentially out of date web servers you visited.",

   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "background": {
      "page":  "background.html" 
   },

   "permissions": ["webRequest", "storage", "<all_urls>"],
   "options_ui": {
		"page" : "options.html",
		"chrome_style" : true
	}
}
